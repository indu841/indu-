#include<stdio.h>
int main(){
    int i,j,temp,k;
    scanf("%d",&k);
    int a[10]={101,44,34,8,10,23,1,9,7};
    for(int i=0;i<10;i++)
    {
        for(j=i+1;j<10;j++)
        if(a[j]>a[i])
        temp=a[i];
        a[i]=a[j];
        a[j]=temp;
    }
{
printf("the largest element is:%d",k,a[-1]);
printf("smallest number is:%d",a);
printf("%d\n",a[i]);
}
return 0;
}
